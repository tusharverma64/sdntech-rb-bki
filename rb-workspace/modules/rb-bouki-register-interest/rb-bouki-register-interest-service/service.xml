<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.3.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_3_0.dtd">

<service-builder dependency-injector="ds" package-path="com.rb.bouki.register.interest">
	<namespace>rb_bouki</namespace>
	<entity local-service="true" name="BoukiRegisterInterest" remote-service="false" >

		<!-- PK fields -->

		<column name="id" primary="true" type="long" />

		<!-- Other fields -->
        <column name="countryCode" type="String" />
		<column name="phoneNo" type="String" />
		<column name="emailAddress" type="String" />
		<column name="keepInform" type="boolean" />
		
		<!-- Audit fields -->

		<column name="createDate" type="Date" />
		<column name="groupId" type="long" />
		<column name="companyId " type="long" />

	</entity>
</service-builder>