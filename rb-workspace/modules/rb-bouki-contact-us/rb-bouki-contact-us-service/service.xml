<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.3.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_3_0.dtd">

<service-builder dependency-injector="ds" package-path="com.rb.bouki.contact.us">
	<namespace>rb_bouki</namespace>
	<entity local-service="true" name="BoukiContactUs" remote-service="false" >

		<!-- PK fields -->

		<column name="id" primary="true" type="long" />

		<!-- Other fields -->

		<column name="name" type="String" />
		<column name="subject" type="String" />
		<column name="emailAddress" type="String" />
		<column name="message" type="String" />
		<column name="apiResponse" type="String" />
		
		<!-- Audit fields -->

		<column name="createDate" type="Date" />
		<column name="groupId" type="long" />
		<column name="companyId " type="long" />
	</entity>
</service-builder>